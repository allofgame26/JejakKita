{
    "name": "laravel/laravel",
    "type": "project",
    "description": "The skeleton application for the Laravel framework.",
    "keywords": ["laravel", "framework"],
    "license": "MIT",
    "require": {
        "php": "^8.1",
        "althinect/filament-spatie-roles-permissions": "^2.3",
        "backstage/filament-2fa": "^3.0",
        "barryvdh/laravel-dompdf": "^3.1",
        "bezhansalleh/filament-shield": "^3.3",
        "dedoc/scramble": "^0.12.35",
        "dvarilek/filament-table-select": "^2.0",
        "filament/filament": "3.3",
        "filament/infolists": "3.3",
        "filament/spatie-laravel-media-library-plugin": "3.0-stable",
        "flowframe/laravel-trend": "^0.4.0",
        "guzzlehttp/guzzle": "^7.2",
        "ibrahim-bougaoua/filaprogress": "^0.1.0",
        "ibrahimbougaoua/filament-rating-star": "^1.0",
        "joaopaulolndev/filament-edit-profile": "^1.0",
        "laravel/fortify": "^1.30",
        "laravel/framework": "^10.10",
        "laravel/jetstream": "^4.3",
        "laravel/sanctum": "^3.3",
        "laravel/tinker": "^2.8",
        "livewire/livewire": "^3.0",
        "maatwebsite/excel": "^3.1",
        "pusher/pusher-php-server": "^7.2",
        "pxlrbt/filament-excel": "*",
        "rupadana/filament-api-service": "3.0.8",
        "spatie/laravel-medialibrary": "^10.15",
        "spatie/laravel-permission": "^6.21"
    },
    "require-dev": {
        "fakerphp/faker": "^1.9.1",
        "laravel/pint": "^1.0",
        "laravel/sail": "^1.18",
        "mockery/mockery": "^1.4.4",
        "nunomaduro/collision": "^7.0",
        "phpunit/phpunit": "^10.1",
        "spatie/laravel-ignition": "^2.0"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi",
            "@php artisan filament:upgrade"
        ],
        "post-update-cmd": [
            "@php artisan vendor:publish --tag=laravel-assets --ansi --force"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ]
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "php-http/discovery": true
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
